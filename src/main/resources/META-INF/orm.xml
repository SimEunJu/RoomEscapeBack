<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings
        xmlns="http://java.sun.com/xml/ns/persistence/orm"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
        version="2.0">

    <sql-result-set-mapping name="storeCommentResultMap">
        <entity-result entity-class="com.sej.escape.entity.comment.StoreComment" />
        <column-result name="is_good_chk"/>
    </sql-result-set-mapping>
    
    <sql-result-set-mapping name="storeResultMap">
        <entity-result entity-class="com.sej.escape.entity.Store" />
        <column-result name="root_path"/>
        <column-result name="sub_path"/>
        <column-result name="star_avg"/>
        <column-result name="zim_cnt"/>
    </sql-result-set-mapping>
    <sql-result-set-mapping name="storeWithAuthResultMap">
        <entity-result entity-class="com.sej.escape.entity.Store" />
        <column-result name="root_path"/>
        <column-result name="sub_path"/>
        <column-result name="star_avg"/>
        <column-result name="zim_cnt"/>
        <column-result name="is_zim_chk"/>
    </sql-result-set-mapping>
    
</entity-mappings>